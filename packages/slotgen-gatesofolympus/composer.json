{
    "name": "slotgen/slotgen-gatesofolympus",
    "description": "Gates Of Olympus plugin for Filament",
    "keywords": [
        "slotgen",
        "laravel",
        "slotgen-gatesofolympus"
    ],
    "homepage": "https://github.com/slotgen/slotgen-gatesofolympus_fila",
    "support": {
        "issues": "https://github.com/slotgen/slotgen-gatesofolympus_fila/issues",
        "source": "https://github.com/slotgen/slotgen-gatesofolympus_fila"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "SlotGen",
            "email": "contact@slotgen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "filament/forms": "^3.0",
        "filament/tables": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0",
        "yepsua/filament-range-field": "^0.3.4"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Slotgen\\SlotgenGatesOfOlympus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slotgen\\SlotgenGatesOfOlympus\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slotgen\\SlotgenGatesOfOlympus\\SlotgenGatesOfOlympusServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
